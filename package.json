{
  "name": "@imbueplatform/dot-http",
  "version": "0.0.0",
  "description": "Imbue Dot Network Http Package",
  "bin": {
    "dot-http": "./bin/http"
  },
  "scripts": {
    "postinstall": "npx tsc",
    "prepublishOnly": "npx tsc",
    "imbue:test": "npx tsc && NODE_ENV=test LOG_LEVEL=off jest test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:imbueplatform/dot-http.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "dependencies": {
    "@types/node": "^13.11.1",
    "@types/pump": "^1.1.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.6",
    "@types/helmet": "^0.0.45",
    "@types/morgan": "1.7.35",
    "@imbueplatform/dot-cell": "^0.0.20",
    "@types/lodash": "^4.14.150",
    "typescript": "^3.8.3",
    "pump": "^3.0.0",
    "debug": "^4.1.1",
    "commander": "^5.0.0",
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "tslint": "^6.1.1",
    "tslint-jest": "1.0.5",
    "ts-jest": "^25.3.0",
    "tslint-config-prettier": "1.18.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "figlet": "^1.3.0"
  },
  "lint-staged": {
    "*.js": [
      "tslint --format codeframe --fix",
      "git add"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "cell",
    "imbue",
    "imbueplatform"
  ],
  "author": "Sasa Savic, <sasa@imbue.app>",
  "license": "MIT"
}
